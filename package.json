{
  "name": "trac-uach",
  "version": "1.0.0",
  "private": false,
  "repository": "https://github.com/PabloSzx/trac-refactor",
  "license": "MIT",
  "author": "PabloSz <pablosaez1995@gmail.com>",
  "main": "index.ts",
  "scripts": {
    "build": "tsc --noEmit && next build",
    "dev": "cross-env NODE_ENV=development concurrently \"yarn dev-api\" \"wait-on tcp:4000 && next\"",
    "dev-api": "ts-node-dev -r dotenv/config --ignore-watch node_modules --project tsconfig.api.json -r dotenv/ -r tsconfig-paths/register --inspect=127.0.0.1:9231 -- api/index.ts",
    "start": "cross-env NODE_ENV=production concurrently \"wait-on tcp:4000 && next start\" \"yarn start-api\"",
    "start-api": "ts-node --project tsconfig.api.json -r tsconfig-paths/register -r dotenv/config api/index.ts"
  },
  "dependencies": {
    "apollo-server-express": "^2.9.7",
    "class-validator": "^0.11.0",
    "connect": "^3.7.0",
    "cookie-parser": "^1.4.4",
    "date-fns": "^2.7.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "http-proxy-middleware": "^0.20.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.1",
    "ms": "^2.1.2",
    "next": "^9.1.3",
    "pg": "^7.12.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^0.17.5"
  },
  "devDependencies": {
    "@types/concurrently": "^4.1.0",
    "@types/connect": "^3.4.32",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.2",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/ms": "^0.7.31",
    "@types/node": "^12.12.7",
    "@types/pg": "^7.11.2",
    "@types/prettier": "^1.18.3",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/wait-on": "^3.2.0",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "prettier": "^1.19.1",
    "ts-node": "^8.5.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.2",
    "wait-on": "^3.3.0"
  }
}
