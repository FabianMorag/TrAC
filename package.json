  "main": "index.ts",
  "scripts": {
    "build": "tsc --noEmit && next build",
    "dev": "cross-env NODE_ENV=development concurrently \"yarn dev-api\" \"wait-on tcp:4000 && next\"",
    "dev-api": "ts-node-dev -r dotenv/config --ignore-watch node_modules --project tsconfig.api.json -r dotenv/ -r tsconfig-paths/register --inspect=127.0.0.1:9231 -- api/index.ts",
    "start": "cross-env NODE_ENV=production concurrently \"wait-on tcp:4000 && next start\" \"yarn start-api\"",
    "start-api": "ts-node --project tsconfig.api.json -r tsconfig-paths/register -r dotenv/config api/index.ts"
  },
